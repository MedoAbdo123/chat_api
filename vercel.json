{
    "vercel": 2,
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "methods": [
                "GET",
                "POST",
                "DELETE",
                "PATCH"
            ],
            "dest": "src/main.ts"
        }
    ]
}
