{
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "methods": [
                "GET",
                "POST",
                "DELETE",
                "PATCH"
            ],
            "dest": "dist/main.js"
        }
    ],
    "buildCommand": "npm run build"
}